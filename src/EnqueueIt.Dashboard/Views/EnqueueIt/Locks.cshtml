@using EnqueueIt
@model List<DistributedLockItem>
@{
    ViewData["Title"] = "Distributed Locks";
}
<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-sm-9">
        <table class="table mt-2">
            <thead>
                <tr>
                    <th>Lock Id</th>
                    <th>Key</th>
                    <th>Created At</th>
                    <th>Last Activity</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var lck in Model)
                {
                    <tr>
                        <td>@lck.Id</td>
                        <td>@lck.Key</td>
                        <td>@lck.StartedAt</td>
                        <td>@lck.LastActivity</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>